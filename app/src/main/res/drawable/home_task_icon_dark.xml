<?xml version="1.0" encoding="utf-8"?>
<animated-selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:id="@+id/home_task_incompleted_d"
        android:drawable="@drawable/home_task_incomplete_dark"
        android:state_enabled="true" />
    <item
        android:id="@+id/home_task_completed_d"
        android:drawable="@drawable/home_task_complete_dark" />
    <transition
        android:drawable="@drawable/home_task_enable_dark"
        android:fromId="@id/home_task_incompleted_d"
        android:toId="@id/home_task_completed_d" />
    <transition
        android:drawable="@drawable/home_task_disable_dark"
        android:fromId="@id/home_task_completed_d"
        android:toId="@id/home_task_incompleted_d" />
</animated-selector>