<?xml version="1.0" encoding="utf-8"?>
<animated-selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:id="@+id/home_ham_open"
        android:drawable="@drawable/home_ham_open"
        android:state_enabled="true"/>
    <item
        android:id="@+id/home_ham_close"
        android:drawable="@drawable/home_ham_close"/>

    <transition
        android:drawable="@drawable/home_ham_openanim"
        android:fromId="@id/home_ham_open"
        android:toId="@id/home_ham_close" />
    <transition
        android:drawable="@drawable/home_ham_closeanim"
        android:fromId="@id/home_ham_close"
        android:toId="@id/home_ham_open" />
</animated-selector>