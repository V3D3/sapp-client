<?xml version="1.0" encoding="utf-8"?>
<animated-selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:id="@+id/nav_timetable_enabled"
        android:drawable="@drawable/nav_timetable_active"
        android:state_enabled="true" />
    <item
        android:id="@+id/nav_timetable_disabled"
        android:drawable="@drawable/nav_timetable_inactive" />

    <transition
        android:drawable="@drawable/nav_timetable_enable"
        android:fromId="@id/nav_timetable_disabled"
        android:toId="@id/nav_timetable_enabled" />
    <transition
        android:drawable="@drawable/nav_timetable_disable"
        android:fromId="@id/nav_timetable_enabled"
        android:toId="@id/nav_timetable_disabled" />
</animated-selector>