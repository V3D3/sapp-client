<?xml version="1.0" encoding="utf-8"?>
<animated-selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:id="@+id/nav_people_enabled"
        android:drawable="@drawable/nav_people_active"
        android:state_enabled="true" />
    <item
        android:id="@+id/nav_people_disabled"
        android:drawable="@drawable/nav_people_inactive" />

    <transition
        android:drawable="@drawable/nav_people_enable"
        android:fromId="@id/nav_people_disabled"
        android:toId="@id/nav_people_enabled" />
    <transition
        android:drawable="@drawable/nav_people_disable"
        android:fromId="@id/nav_people_enabled"
        android:toId="@id/nav_people_disabled" />
</animated-selector>