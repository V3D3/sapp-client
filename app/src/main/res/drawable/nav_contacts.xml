<?xml version="1.0" encoding="utf-8"?>
<animated-selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:id="@+id/nav_contacts_enabled"
        android:drawable="@drawable/nav_contacts_active"
        android:state_enabled="true" />
    <item
        android:id="@+id/nav_contacts_disabled"
        android:drawable="@drawable/nav_contacts_inactive" />

    <transition
        android:drawable="@drawable/nav_contacts_enable"
        android:fromId="@id/nav_contacts_disabled"
        android:toId="@id/nav_contacts_enabled" />
    <transition
        android:drawable="@drawable/nav_contacts_disable"
        android:fromId="@id/nav_contacts_enabled"
        android:toId="@id/nav_contacts_disabled" />
</animated-selector>