<?xml version="1.0" encoding="utf-8"?>
<animated-selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:id="@+id/home_task_incompleted"
        android:drawable="@drawable/home_task_incomplete"
        android:state_enabled="true" />
    <item
        android:id="@+id/home_task_completed"
        android:drawable="@drawable/home_task_complete" />

    <transition
        android:drawable="@drawable/home_task_enable"
        android:fromId="@id/home_task_incompleted"
        android:toId="@id/home_task_completed" />
    <transition
        android:drawable="@drawable/home_task_disable"
        android:fromId="@id/home_task_completed"
        android:toId="@id/home_task_incompleted" />
</animated-selector>