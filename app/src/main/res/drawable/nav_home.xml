<?xml version="1.0" encoding="utf-8"?>
<animated-selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:id="@+id/nav_home_enabled"
        android:drawable="@drawable/nav_home_active"
        android:state_enabled="true" />
    <item
        android:id="@+id/nav_home_disabled"
        android:drawable="@drawable/nav_home_inactive" />

    <transition
        android:drawable="@drawable/nav_home_enable"
        android:fromId="@id/nav_home_disabled"
        android:toId="@id/nav_home_enabled" />
    <transition
        android:drawable="@drawable/nav_home_disable"
        android:fromId="@id/nav_home_enabled"
        android:toId="@id/nav_home_disabled" />
</animated-selector>